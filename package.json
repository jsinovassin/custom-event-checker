{
  "name": "custom-event-checker",
  "version": "0.1.0",
  "description": "Script that check events coming from a jCustomer to another one",
  "author": "SINOVASSIN-NAIK Jonathan",
  "bin": {
    "custom-event-identifier": "./bin/run"
  },
  "homepage": "https://github.com/jsinovassin/custom-event-checker",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "jsinovassin/custom-event-checker",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/core": "^2",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.4.7",
    "axios": "^1.4.0"
  },
  "devDependencies": {
    "@oclif/test": "^2.3.17",
    "@types/chai": "^4",
    "@types/fs-extra": "^11.0.1",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.18.25",
    "chai": "^4",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.3",
    "mocha": "^9",
    "oclif": "^3",
    "shx": "^0.3.4",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5"
  },
  "oclif": {
    "bin": "custom-event-checker",
    "dirname": "custom-event-checker",
    "commands": "./dist/commands",
    "macos": {
      "identifier": "org.jahia.cli"
    },
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "validateEvents": {
        "description": "Script that help to identify unknown properties of custom events"
      }
    }
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "version": "oclif readme && git add README.md"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}
